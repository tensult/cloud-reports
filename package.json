    "finalhandler": "^1.1.1",
    "gulp": "^4.0.1",
    "node-fetch": "^2.1.2",
    "opn": "^5.3.0",
    "serve-static": "^1.13.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "aws-sdk": "^2.328.0",
    "cpy": "^7.0.1",
    "ejs": "^2.6.1",
